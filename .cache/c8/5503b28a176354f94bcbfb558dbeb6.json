r\n\tborder-style: border-box;\r\n\t-webkit-appearance: none;\r\n\twidth: $track-width;\r\n\theight: $track-height;\r\n\tbackground: $track-background-color;\r\n\tborder-radius: $track-border-radius;\r\n\ttransition: 0.4s;\r\n\topacity: 1;\r\n\r\n\t&:hover {\r\n\t\twidth: $track-width + 5%;\r\n\t}\r\n\r\n\t&::-webkit-slider-thumb {\r\n\t\t-webkit-appearance: none;\r\n\t\tcursor: pointer;\r\n\t\tbox-shadow: 6px 6px 13px rgba(0, 0, 0, 0.75);\r\n\t\topacity: 0.6;\r\n\t\ttransition: 0.2s;\r\n\t\tborder-radius: $thumb-border-radius;\r\n\t\tbackground: $thumb-color;\r\n\t\theight: $thumb-height;\r\n\t\twidth: $thumb-width;\r\n\r\n\t\t&:hover {\r\n\t\t\topacity: 1;\r\n\t\t}\r\n\t}\r\n\r\n\t&:focus {\r\n\t\toutline: none;\r\n\t}\r\n}\r\n\r\n.largeNumberRange {\r\n\tmargin-bottom: 30px;\r\n}\r\n\r\n.smallNumberRange {\r\n\tmargin-bottom: 20px;\r\n}\r\n\r\n.proceed_BTN {\r\n\tmargin: 0;\r\n\tpadding: 1.25% 4%;\r\n\t@include text-style(25px);\r\n\tbackground: #1bc223;\r\n\tborder: 0;\r\n\tborder-radius: 5px;\r\n\tbox-shadow: 11px 13px 22px rgba(0, 0, 0, 0.77);\r\n\ttransition: 0.1s;\r\n\r\n\t&:active {\r\n\t\ttransform: translateY(1.5px);\r\n\t\topacity: 0.9;\r\n\t}\r\n\r\n\t&:focus {\r\n\t\toutline: none;\r\n\t}\r\n}\r\n\r\n.mainScreen {\r\n\tposition: absolute;\r\n\ttop: 50%;\r\n\tleft: 50%;\r\n\ttransform: translate(-50%, -50%);\r\n\t@include text-style(25px);\r\n\tz-index: 1;\r\n}\r\n\r\n.numbersList {\r\n\tdisplay: flex;\r\n\tflex-direction: row;\r\n\tflex-wrap: no-wrap;\r\n\tjustify-content: center;\r\n\tmargin: 0;\r\n\r\n\t.listItem {\r\n\t\t@include text-style(50px);\r\n\t\tbackground-color: #333;\r\n\t\tpadding: 30px 50px;\r\n\t\tmargin: 10px;\r\n\t\tborder-radius: 15px;\r\n\t\ttext-align: center;\r\n\t}\r\n}\r\n\r\n@media (max-width: 1000px) {\r\n\t.numbersList {\r\n\t\tflex-wrap: wrap;\r\n\t}\r\n}\r\n\r\n.showTargetNumber_BTN {\r\n\tdisplay: block; // change between block and none\r\n\tmargin: 15% auto 0 auto;\r\n\tpadding: 1.25% 4%;\r\n\t@include text-style(25px);\r\n\tbackground: #1bc223;\r\n\tborder: 0;\r\n\tborder-radius: 5px;\r\n\tbox-shadow: 11px 13px 22px rgba(0, 0, 0, 0.77);\r\n\ttransition: 0.1s;\r\n\r\n\t&:active {\r\n\t\ttransform: translateY(1.5px);\r\n\t\topacity: 0.9;\r\n\t}\r\n\r\n\t&:focus {\r\n\t\toutline: none;\r\n\t}\r\n}\r\n\r\n.targetNumber {\r\n\tdisplay: none; // change between block and none\r\n\tmargin: 15% auto 0 auto;\r\n\tpadding: 1.25% 4%;\r\n\twidth: 90px;\r\n\t@include text-style(50px);\r\n\tbackground: #1bc223;\r\n\tborder: 0;\r\n\tborder-radius: 5px;\r\n\tbox-shadow: 11px 13px 22px rgba(0, 0, 0, 0.77);\r\n}\r\n\r\n.reload_BTN {\r\n\tposition: fixed;\r\n\tleft: 50%;\r\n\tbottom: 0px;\r\n\ttransform: translate(-50%, -50%);\r\n\tmargin: 0 auto;\r\n\r\n\tbackground-color: transparent;\r\n\tborder: 0;\r\n\tcolor: #333;\r\n\ttransition: 0.1s;\r\n\r\n\t&:active {\r\n\t\topacity: 0.9;\r\n\t}\r\n\r\n\t&:focus {\r\n\t\toutline: none;\r\n\t}\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"25b19df3d9b87bbe18caad13ba715567","cacheData":{"env":{}}}