rated":{"line":212,"column":2}},{"source":"pages/easyMode/src/style.scss","name":null,"original":{"line":226,"column":1},"generated":{"line":213,"column":2}},{"source":"pages/easyMode/src/style.scss","name":null,"original":{"line":227,"column":1},"generated":{"line":214,"column":2}},{"source":"pages/easyMode/src/style.scss","name":null,"original":{"line":228,"column":1},"generated":{"line":215,"column":2}},{"source":"pages/easyMode/src/style.scss","name":null,"original":{"line":230,"column":1},"generated":{"line":217,"column":0}},{"source":"pages/easyMode/src/style.scss","name":null,"original":{"line":231,"column":2},"generated":{"line":218,"column":2}}],"sources":{"pages/easyMode/src/style.scss":"/*\n  .welcomeScreen\n  (.largeNumberRange\n  .smallNumberRange)\n  .rangeStyle\n  .largeNumbersOutput\n  .smallNumbersOutput\n  .proceed_BTN\n\n  .mainScreen\n  .numbersList\n  .listItem\n  .showTargetNumber_BTN\n  .targetNumber\n  .reload_BTN\n*/\n\n@mixin text-style($font-size, $text-align: center) {\n\tfont-family: sans-serif;\n\ttext-align: $text-align;\n\tcolor: #fcfcfc;\n\tfont-size: $font-size;\n}\n\n$background-color: #181818;\n$main-color: #1b92e2;\n\nbody {\n\tmargin: 0;\n\tpadding: 0;\n\tbackground: $background-color;\n}\n\n.welcomeScreen {\n\tposition: fixed;\n\tdisplay: block; // Change to show or hide welcome screen\n\twidth: 100%;\n\theight: 100%;\n\ttop: 0;\n\tleft: 0;\n\tright: 0;\n\tbottom: 0;\n\tz-index: 100;\n\tpadding-top: 10%;\n\tbackground: $background-color;\n\t@include text-style(20px);\n\n\th1 {\n\t\t@include text-style(58px);\n\t}\n}\n\n/* Range Styling */\n$track-width: 65%;\n$track-height: 42px;\n$track-background-color: #333333;\n$track-border-radius: 15px;\n\n$thumb-color: $main-color;\n$thumb-height: 55px;\n$thumb-width: 30px;\n$thumb-border-radius: 8px;\n\ninput[type='range'] {\n\tborder-style: border-box;\n\t-webkit-appearance: none;\n\twidth: $track-width;\n\theight: $track-height;\n\tbackground: $track-background-color;\n\tborder-radius: $track-border-radius;\n\ttransition: 0.4s;\n\topacity: 1;\n\n\t&:hover {\n\t\twidth: $track-width + 5%;\n\t}\n\n\t&::-webkit-slider-thumb {\n\t\t-webkit-appearance: none;\n\t\tcursor: pointer;\n\t\tbox-shadow: 6px 6px 13px rgba(0, 0, 0, 0.75);\n\t\topacity: 0.8;\n\t\ttransition: 0.2s;\n\t\tborder-radius: $thumb-border-radius;\n\t\tbackground: $thumb-color;\n\t\theight: $thumb-height;\n\t\twidth: $thumb-width;\n\n\t\t&:hover {\n\t\t\topacity: 1;\n\t\t}\n\t}\n\n\t&:focus {\n\t\toutline: none;\n\t}\n}\n\n.largeNumberRange {\n\tmargin-bottom: 30px;\n}\n\n.smallNumberRange {\n\tmargin-bottom: 20px;\n}\n\n.proceed_BTN {\n\tmargin: 0;\n\tpadding: 1% 2.5%;\n\t@include text-style(45px);\n\tbackground: $main-color;\n\tborder: 0;\n\tborder-radius: 5px;\n\tbox-shadow: 11px 13px 22px rgba(0, 0, 0, 0.77);\n\ttransition: 0.1s;\n\n\t&:active {\n\t\ttransform: translateY(1.5px);\n\t\topacity: 0.9;\n\t}\n\n\t&:focus {\n\t\toutline: none;\n\t}\n}\n\n.mainScreen {\n\tposition: absolute;\n\ttop: 50%;\n\tleft: 50%;\n\ttransform: translate(-50%, -50%);\n\t@include text-style(25px);\n\tz-index: 1;\n}\n\n.numbersList {\n\tdisplay: flex;\n\tflex-direction: row;\n\tflex-wrap: no-wrap;\n\tjustify-content: center;\n\tmargin: 0;\n\n\t.listItem {\n\t\t@include text-style(50px);\n\t\tbackground-color: #333;\n\t\tpadding: 30px 50px;\n\t\tmargin: 10px;\n\t\tborder-radius: 15px;\n\t\ttext-align: center;\n\t}\n}\n\n@media (max-width: 1000px) {\n\t.numbersList {\n\t\tflex-wrap: wrap;\n\t}\n}\n\n@media (max-width: 760px) {\n\t.numbersList .listItem {\n\t\t@include text-style(25px);\n\t\tpadding: 15px 25px;\n\t}\n}\n\n.showTargetNumber_BTN {\n\tdisplay: block; // change between block and none\n\tmargin: 15% auto 0 auto;\n\tpadding: 1.25% 4%;\n\t@include text-style(45px);\n\tbackground: $main-color;\n\tborder: 0;\n\tborder-radius: 5px;\n\tbox-shadow: 11px 13px 22px rgba(0, 0, 0, 0.77);\n\ttransition: 0.1s;\n\n\t&:active {\n\t\ttransform: translateY(1.5px);\n\t\topacity: 0.9;\n\t}\n\n\t&:focus {\n\t\toutline: none;\n\t}\n}\n\n.targetNumber {\n\tdisplay: none; // change between block and none\n\tmargin: 15% auto 0 auto;\n\tpadding: 1.25% 4%;\n\twidth: 90px;\n\t@include text-style(50px);\n\tbackground: $main-color;\n\tborder: 0;\n\tborder-radius: 5px;\n\tbox-shadow: 11px 13px 22px rgba(0, 0, 0, 0.77);\n}\n\n.reload_BTN {\n\tposition: fixed;\n\tleft: 50%;\n\tbottom: 25px;\n\ttransform: translate(-50%, -50%);\n\tmargin: 0 auto;\n\n\tbackground-color: transparent;\n\tborder: 0;\n\tcolor: #333;\n\ttransition: 0.1s;\n\n\t&:active {\n\t\topacity: 0.9;\n\t}\n\n\t&:focus {\n\t\toutline: none;\n\t}\n}\n\n.back_BTN {\n\tz-index: 100;\n\tposition: fixed;\n\tpadding: 5px;\n\ttop: 0;\n\n\tbackground-color: transparent;\n\tborder: 0;\n\tcolor: #fcfcfc;\n\n\t.material-icons {\n\t\tfont-size: 35px;\n\t}\n}\n"},"lineCount":null}},"error":null,"hash":"c46c9b1b8f2f21a9529603bf84f37663","cacheData":{"env":{}}}